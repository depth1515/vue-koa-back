  "main": "index.js",
  "scripts": {
    "start": "npx nodemon --exec babel-node src/index.js",
    "webpack:debug": "npx node --inspect-brk ./node_modules/webpack/bin/webpack --inline --progress",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js",
    "dev:dev": "cross-env NODE_ENV=dev nodemon --exec babel-node --inspect ./src/index.js",
    "clean": "rimraf dist",
    "watch": "cross-env NODE_ENV=dev webpack --watch --progress --stats-error-details --config config/webpack.config.dev.js",
    "debug": "nodemon --inspect ./dist/server.bundle.js",
    "dev": "npm-run-all -p watch debug"
  },
